{
    "manifestVersion": 1,
    "id": "code-management",
    "publisher": "llima",
    "version": "1.0.0",
    "name": "Code Management",
    "description": "Code management extension for Azure DevOps",
    "public": true,
    "categories": [
        "Azure Boards"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "azure/icon.png",
        "branding": "azure/icon.png"
    },
    "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "azure/license.md"
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/llima/code-management-extension"
    },
    "links": {
        "support": {
            "url": "mailto:luiz.lima@live.com"
        }
    },
    "scopes": [
        "vso.work"
    ],
    "tags": [
        "Work Item",
        "Code Management",
        "Branch",
        "Repo"
    ],
    "contributions": [
        {
            "id": "code-management-feature",
            "type": "ms.vss-web.feature",
            "description": "Enable/Disable all Code Management features",
            "targets": [
                "ms.vss-web.managed-features"
            ],
            "properties": {
                "name": "Code Management",
                "hostConfigurable": true,
                "defaultState": true,
                "hostScopes": [
                    "project"
                ]
            }
        }
    ],
    "files": [
        {
            "path": "azure",
            "addressable": true
        },
        {
            "path": "feature/build",
            "addressable": true
        }
    ]
}